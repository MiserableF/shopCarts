}

.share_box .count_box .count_float .float_one {
    clear: both;
    width: 80rpx;
    height: 32rpx;
    font-size: 0;
    padding-bottom: 10rpx;
    position: relative;
}

.share_box .count_box .count_float .float_one view {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    color: #fff;
    font-size: 20rpx;
    width: 80rpx;
    line-height: 32rpx;
    text-align: center;
    z-index: 999;
}

.share_box .count_box .count_float .float_one image {
    width: 80rpx;
    height: 32rpx;
}

.share_box .count_box .count_float .float_two {
    width: 80rpx;
    height: 80rpx;
    font-size: 0;
}

.share_box .count_box .count_float .float_two image {
    width: 76rpx;
    height: 76rpx;
    border: 2rpx solid #ff436e;
    border-radius: 40rpx;
}

.share_box .count_box .count_float .float_three {
    width: 80rpx;
    height: 80rpx;
    font-size: 0;
}

.share_box .count_box .count_float .float_three image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 40rpx;
}

.share_box button {
    margin: 0 55rpx;
    width: 600rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0;
    font-size: 28rpx;
    background: #ff436e;
    color: #fff;
    text-align: center;
    border-radius: 40rpx;
}

