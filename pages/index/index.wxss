}

.bd-bs {
    display: flex;
    background: #fff;
    font-size: 28rpx;
    border-top: 2rpx solid #f0f0f0;
    border-bottom: 2rpx solid #f0f0f0;
    padding: 20rpx;
    margin-bottom: 20rpx;
}

.bd-bs .brand {
    width: 354rpx;
