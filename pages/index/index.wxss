}

.share_box {
    background: #fff;
    padding: 20rpx;
}

.share_box .cut_rule {
    float: right;
    width: 100rpx;
    color: #ff436e;
    padding: 12rpx;
    border: 2rpx solid #ff436e;
    border-radius: 8rpx;
    margin-bottom: 20rpx;
}

.share_box .count_box {
    clear: both;
    padding: 0 55rpx 20rpx;
